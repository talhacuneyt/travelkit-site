{
  "version": 2,
  "functions": {
    "api/contact.js": {
      "maxDuration": 10
    },
    "api/messages.js": {
      "maxDuration": 10
    },
    "api/packages.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.travelkit.com.tr"
        }
      ],
      "destination": "https://travelkit.com.tr/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "travelkit-site.vercel.app"
        }
      ],
      "destination": "https://travelkit.com.tr/$1",
      "permanent": true
    }
  ]
}